{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://kropp-gym.netlify.app",
        "Access-Control-Allow-Methods": "GET, POST",
        "Access-Control-Allow-headers": "Accept, Content-Type"
      }
    }
  ]
}
